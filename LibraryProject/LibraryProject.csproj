    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\navBarImg.png" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="DB\" />
